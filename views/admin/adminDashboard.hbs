				borderColor: 'rgb(255, 99, 132)',
				data: [{{#each paymentGraph}}{{totalAmount}},{{/each}}],
			}]
		};

		const config = {
			type: 'bar',
			data: data,
			options: {}
		};


		const myChart = new Chart(
			document.getElementById('myChart'),
			config
		);
	</script>

